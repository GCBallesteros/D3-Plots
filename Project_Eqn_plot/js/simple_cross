class SimpleCross
  width: 600
  height: 400

  margin:
    top: 70
    right: 70
    bottom: 70
    left: 70

  nx: 0

  ny: 0


  plot: ->
    w = @width - @margin.left - @margin.right
    h = @height - @margin.top - @margin.bottom

    xScale = d3.scale.linear().range([0, w])
    yScale = d3.scale.linear().range([h, 0])
    #zScale = d3.scale.linear().range(["green","white", "blue"])

 
